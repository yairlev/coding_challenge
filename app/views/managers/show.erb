<h1>Staff Members</h1>
<table class="table table-hover">
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone Number</th>
    <th>Action</th>
  </tr>
  </thead>
  <% @user.staff_members.each do |staff_member| %>
    <%= render :partial => "shared/user", :locals => {:user => staff_member} %>
  <% end %>
</table>

<%= link_to "Add Staff Member", new_staff_member_path %>